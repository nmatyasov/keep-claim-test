{
  "name": "keep-claim-test",
  "version": "1.0.0",
  "description": "Тестововое задание Backed-разработчик",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "test:coverage": "jest --coverage --config ./jest.config.js",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**' --ext 'ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "keywords": [
    "keep-claim",
    "backend",
    "typescript"
  ],
  "author": "Nikolay Matyasov",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
